  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest --coverage --ui",
    "lint": "eslint .",
    "typegen": "tsx scripts/typegen.ts",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@angular-eslint/eslint-plugin": "^21.1.0",
